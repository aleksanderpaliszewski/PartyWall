{
  "name": "PartyWall",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit --project . && eslint . --ext .js,.jsx,.ts,.tsx",
    "server": "json-server --watch ./db.json",
    "server-auth": "node server.js"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "enzyme": "^3.11.0",
    "formik": "^2.2.6",
    "jsonwebtoken": "^8.5.1",
    "qs": "^6.9.6",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-paper": "^4.6.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-vector-icons": "^7.1.0",
    "url-join": "^4.0.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-async-helpers": "^0.9.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.14.20",
    "@types/qs": "^6.9.5",
    "@types/react-native": "^0.63.40",
    "@types/react-test-renderer": "^16.9.2",
    "@types/url-join": "^4.0.0",
    "babel-jest": "^25.1.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.5.1",
    "husky": "^4.3.7",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "json-server": "^0.16.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "^17.0.1",
    "react-native-dotenv": "^2.5.0",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./__tests__/setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "@testing-library/jest-native/extend-expect"
    ],
    "testRegex": "(/__tests__/.*-test.(ts|tsx))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation|react-native-barcode-builder)/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
